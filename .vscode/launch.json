{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Expo Web (Chrome)",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8081",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
                "webpack:///./*": "${workspaceFolder}/*",
                "webpack:///*": "*"
            }
        },
        {
            "name": "Attach to Chrome",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true
        }
    ]
}