{
  "expo": {
    "name": "My_Compass",
    "slug": "My_Compass",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mycompass",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "mil.navy.mycompass",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Required for event check-in",
        "NSPhotoLibraryUsageDescription": "My Compass may save images to your photo library.",
        "NSLocationWhenInUseUsageDescription": "My Compass uses your location to find nearby commands and events.",
        "NSCalendarsUsageDescription": "My Compass syncs leave requests to your calendar.",
        "NSRemindersUsageDescription": "My Compass creates reminders for important career milestones.",
        "NSFaceIDUsageDescription": "My Compass uses Face ID to secure your sensitive personnel data.",
        "NSContactsUsageDescription": "My Compass accesses contacts to help you connect with sailors."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "display": "standalone",
      "backgroundColor": "#0A1628",
      "themeColor": "#0A1628"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-notifications",
      "@react-native-community/datetimepicker",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "Required for event check-in"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "90a02736-d91a-49c4-a21f-9844b9f6dda0"
      }
    },
    "owner": "matthewcla"
  }
}
